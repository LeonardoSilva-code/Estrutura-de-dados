package ListaSimpesmenteEncadeada;

public class Node <T extends Comparable<T>>{

    private T dado;
    private Node proximo;


    public Node(){
        proximo = null;
    }

    public Node (T dado){
        this.dado = dado;
        proximo = null;
    }

    public T getDado() {
        return dado;
    }

    public void setDado(T dado) {
        this.dado = dado;
    }

    public Node getProximo() {
        return proximo;
    }

    public void setProximo(Node proximo) {
        this.proximo = proximo;
    }
}
